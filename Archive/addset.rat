#-H- ADDSET.RAT 289 1980 103 2153
INCLUDE MDEF
 
# ADDSET - PUT  C  IN  SET(J)  IF IT FITS,  INCREMENT  J
   INTEGER FUNCTION ADDSET(C, SET, J, MAXSIZ)
   INTEGER J, MAXSIZ
   CHARACTER C, SET(ARB)
 
   IF (J > MAXSIZ)
      ADDSET = NO
   ELSE <
      SET(J) = C
      J = J + 1
      ADDSET = YES
      !
   RETURN
   END
